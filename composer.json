{
    "name": "dameert/pr-stats",
    "authors": [
        {
            "name": "David Meert",
            "email": "david@davidmeert.be"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "knplabs/github-api": "^2.14",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
